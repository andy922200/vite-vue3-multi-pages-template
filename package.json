{
  "name": "vite-vue3-multi-pages-template",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "preview": "vite preview",
    "lint": "run-s run-prettier run-eslint",
    "run-eslint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "run-prettier": "prettier --write src/",
    "test-jest": "jest",
    "test-e2e": "testcafe",
    "clear-all-test-results": "rm -rf coverage screenshots"
  },
  "engines": {
    "node": ">=20.12.0",
    "npm": ">=10.5.0"
  },
  "resolutions": {
    "esbuild": "npm:esbuild-wasm@latest"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@vueuse/core": "^10.10.0",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "pinia": "^2.1.7",
    "sweetalert2": "^11.12.0",
    "swiper": "^11.1.4",
    "vue": "^3.4.27",
    "vue-i18n": "10.0.0-beta.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@iconify/json": "^2.2.216",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "eslint-plugin-vue": "^9.26.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.77.6",
    "tailwindcss": "^3.4.3",
    "testcafe": "^3.6.0",
    "ts-jest": "^29.1.4",
    "typescript": "~5.4.5",
    "unplugin-icons": "^0.19.0",
    "vite": "^5.2.12",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-zip-pack": "^1.2.3",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.0.19"
  }
}
